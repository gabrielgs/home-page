{
  "recognizeSelfClosing": true,
  "lowerCaseAttributeName:" : false,
  "plugins": {
    "posthtml-bem": {
      "elemPrefix": "-",
      "modPrefix": "_",
      "modDlmtr": "--"
    },
    "posthtml-expressions": {
      "locals": {
        "foo": "bar"
      }
    },
    "posthtml-include": {
      "encoding": "utf-8",
      "root": "./src"
    }
  }
}